{
    "name": "mbright/PHP-CRUD",
    "description": "A Basic PHP CRUD application",
    "authors": [
        {
            "name": "Marcus Fulbright"
        }
    ],
    "require": {
      "php": ">=5.5.0",
      "aura/router": "~2.3.0",
      "vlucas/spot2": "~2.0",
      "vlucas/phpdotenv": "~2.0",
      "aura/filter": "2.0.0-beta1",
      "aura/view": "~2.1.0"
    },
    "autoload": {
        "psr-4": {
            "Mbright\\": "src/"
        }
    },
    "require-dev": {
      "mockery/mockery": "~0.9"
    },
    "autoload-dev": {
        "psr-4": {
            "Aura\\Router\\": "tests/",
            "Aura\\Di\\": "vendor/aura/di/tests"
        }
    }
}
